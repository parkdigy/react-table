{
  "name": "@pdg/npm-react-admin-layout-template",
  "title": "NPM React Admin Layout Template",
  "description": "NPM React Admin Layout Template",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parkdigy/npm-react-admin-layout-template.git",
    "baseUrl": "https://github.com/parkdigy/npm-react-admin-layout-template"
  },
  "bugs": {
    "url": "https://github.com/parkdigy/npm-react-admin-layout-template/issues"
  },
  "homepage": "https://github.com/parkdigy/npm-react-admin-layout-template#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "cd examples && npm run dev",
    "watchman:del": "watchman watch-del \"${PWD}\" ; watchman watch-project \"${PWD}\"",
    "build:examples": "cd examples && npm run build",
    "build": "npm run watchman:del && npm run lint && rollup -c --bundleConfigAsCjs",
    "git:commit": "node .git-commit.cjs",
    "git:push": "git push",
    "git:commit:push": "npm run git:commit && npm run git:push",
    "git:merge:mirror": "node .git-merge.cjs mirror main",
    "reset:gitignore": "git rm -r --cached . && git add .",
    "pub": "npm i && npm run build:examples && npm run build && npm publish --access=public && rm ./.git/hooks/pre-commit",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "reinstall": "npm run reinstall:module",
    "reinstall:module": "rm -rf node_modules && rm -f package-lock.json && npm i"
  },
  "author": "YOUNG CHUL PARK",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "util",
    "typescript",
    "javascript"
  ],
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "9.39.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@types/node": "^22.19.2",
    "@types/react": "19.2.7",
    "@typescript-eslint/parser": "8.49.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "^16.5.0",
    "prettier": "3.7.4",
    "rollup": "4.53.3",
    "rollup-plugin-delete": "2.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-sass": "1.15.3",
    "rollup-plugin-typescript2": "0.36.0",
    "sass": "1.95.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0"
  }
}
