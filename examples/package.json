{
  "name": "examples",
  "scripts": {
    "dev": "cross-env NODE_ENV=development LIB_ENV=development webpack serve --config=webpack.config.js",
    "dev-prd-lib": "cross-env NODE_ENV=development LIB_ENV=production webpack serve --config=webpack.config.js",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "clean:prod": "rm -rf build && rm -rf dist && mkdir dist",
    "clean:prod:win": "rmdir /s /q build && rmdir /s /q dist && mkdir dist",
    "copy:index": "cp ./dist/index.html ../index.html && cp ./dist/index.html ../404.html",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "build": "npm run clean:prod && npm run lint && npm run build:webpack && npm run copy:index",
    "reinstall-module": "rm -rf node_modules && rm -f package-lock.json && npm i"
  },
  "devDependencies": {
    "@pdg/react-admin-layout": "^1.0.43",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "classnames": "^2.5.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "react": "^19.1.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-shell-plugin-next": "^2.3.2"
  }
}
