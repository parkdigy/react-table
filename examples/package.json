{
  "name": "examples",
  "browserslist": [
    "extends browserslist-config-baseline"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config=webpack.config.js",
    "clean:prod": "rm -rf build && rm -rf dist && mkdir dist",
    "copy:index": "cp ./dist/index.html ../index.html && cp ./dist/index.html ../404.html",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "build": "npm run clean:prod && npm run lint && npm run build:webpack && npm run copy:index",
    "git:commit": "cd .. && npm run git:commit",
    "git:push": "cd .. && npm run git:push",
    "git:commit:push": "cd .. && npm run git:commit:push",
    "git:merge:mirror": "cd .. && npm run git:merge:mirror",
    "reinstall": "npm run reinstall:module",
    "reinstall:module": "rm -rf node_modules && rm -f package-lock.json && npm i",
    "lint": "eslint src",
    "tsc": "npx tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.28.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.0",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/parser": "^8.33.0",
    "babel-loader": "^10.0.0",
    "baseline-browser-mapping": "^2.8.25",
    "browserslist-config-baseline": "^0.5.0",
    "classnames": "^2.5.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^5.0.1",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "react-router": "^7.6.1",
    "sass": "^1.89.1",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.18",
    "ts-loader": "^9.5.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.33.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
